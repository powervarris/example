@model Student;

<h1>Instructor Details</h1>

<table class="table table-dark">
    <tr>
        <th>Student Id: </th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>First Name: </th>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <th>Course: </th>
        <td>@Model.Course</td>
    </tr>

    <tr>
        <th>Date Enrolled: </th>
        <td>@Model.dateEnrolled</td>
    </tr>

    <tr>
        <th>Email: </th>
        <td>@Model.Email</td>
    </tr>


</table>